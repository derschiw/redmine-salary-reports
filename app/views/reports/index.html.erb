<% html_title t('.reports') %>
<h2><%= t('.reports_title') %></h2>

<%= link_to t(".new_report"), new_report_path %>
<br>
<br>
<table>
  <tbody>
    <% @reports.each do |report| %>
      <tr>
        <td><%= link_to "#{I18n.l(report.date_from, format: '%d. %B %Y')} - #{I18n.l(report.date_to, format: '%d. %B %Y')}", report %></td>
        <td><%= link_to report.user, report.user %></td>
      </tr>
    <% end %>
  </tbody>
</table>
